package ba.bitcamp.w05d05.lectures.thisandsuper;

import java.util.Random;
import java.util.Scanner;

public class IgrajPogodiBroj {

	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		Random rand = new Random();
		
		PogodiBroj pb = new PogodiBroj(100, 5);
		
		
		for (int i = 0; i < pb.maxBrojPokusaja; i++) {
			
			int randomBroj = rand.nextInt(pb.maxBroj);
			System.out.println(randomBroj);
			
		}
		
		
		
		in.close();
		
		
	}
	
	
}
